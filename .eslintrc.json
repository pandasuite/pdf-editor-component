{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-use-before-define": "off",
    "object-curly-newline": "off",
    "operator-linebreak"
  }
}
